## Script para criar entidade onde ser√£o alocados os dados do(s) arquivo(s) das criptomoedas;

CREATE TABLE cripromoedas (
    Id SERIAL PRIMARY KEY,
    Nome VARCHAR(255),
    Simbolo VARCHAR(10),
    Data TIMESTAMP,
    Alto NUMERIC,
    Baixo NUMERIC,
    Aberto NUMERIC,
    Fechado NUMERIC,
    Volume BIGINT,
    Valor_acao NUMERIC
);
